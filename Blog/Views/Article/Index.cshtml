<h1>Articles List</h1>
<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Content</th>
            <th>PublishedDate</th>
            <th>UserId</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var article in Model)
        {
            <tr>
                <td>@article.Title</td>
                <td>@article.Content</td>
                <td>@article.PublishedDate</td>
                <td>@article.UserId</td>
            </tr>
        }
    </tbody>
</table>
